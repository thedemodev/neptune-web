{
  "name": "@transferwise/components",
  "version": "26.0.5",
  "description": "TransferWise styleguide components in react",
  "license": "Apache-2.0",
  "main": "./build/umd/polyfill/main.js",
  "module": "./build/es/polyfill/index.js",
  "style": "./build/main.css",
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune",
    "url": "git+https://github.com/transferwise/neptune-web.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:watch-less": "gulp lessWatcher",
    "dev:storybook": "start-storybook -p 3001",
    "docs": "npm-run-all --parallel docs:*",
    "docs:babel": "NODE_ENV=es babel src -d build/es/polyfill --ignore '**/*.spec.js','**/*.story.js,' -w",
    "docs:watch-less": "gulp lessWatcher",
    "storybook": "start-storybook -p 3001",
    "build": "npm-run-all build:*",
    "build:clean": "rm -rf lib build",
    "build:emotion-fix": "node ./emotionFix.js",
    "build:compile-css": "gulp lessCompiler",
    "build:umd": "NODE_ENV=umd rollup -c",
    "build:umd-nopolyfill": "NODE_ENV=umd-nopolyfill rollup -c",
    "build:es": "NODE_ENV=es babel src -d build/es/polyfill --ignore '**/*.spec.js','**/*.story.js'",
    "build:es-nopolyfill": "NODE_ENV=es-nopolyfill babel src -d build/es/no-polyfill --ignore '**/*.spec.js','**/*.story.js'",
    "build:copy-files": "cpx 'src/**/!(db)/*.{css,json,svg}' build/es/polyfill",
    "build:copy-files2": "cpx 'src/**/!(db)/*.{css,json,svg}' build/es/no-polyfill",
    "test": "gulp lessCompiler && jest --env=jsdom --runInBand && yarn test:bundleSize",
    "test:watch": "jest --watch --env=jsdom",
    "test:bundleSize": "bundlesize",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-react-jsx-self": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.4.5",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-centered": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.3",
    "@transferwise/eslint-config": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "1.6.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-minify": "^0.5.1",
    "bootstrap": "github:transferwise/bootstrap#v5.18.15",
    "bundlesize": "^0.18.0",
    "camelcase": "^5.2.0",
    "cpx": "^1.5.0",
    "cssnano": "^4.1.10",
    "currency-flags": "^1.2.1",
    "deploy-directory-on-branch-to-gh-pages": "^0.2.5",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fs-extra": "8.1.0",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-dependents": "^1.2.3",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-print": "^5.0.2",
    "husky": "^2.7.0",
    "jest": "24.7.1",
    "jest-cli": "^24.8.0",
    "jest-emotion": "^10.0.14",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.4.0",
    "less": "^3.10.3",
    "lint-staged": "^9.4.2",
    "lodash.times": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.1",
    "react-dev-utils": "^9.0.1",
    "react-test-renderer": "16.8.6",
    "resolve": "1.12.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-gzip": "^2.3.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "peerDependencies": {
    "bootstrap": "github:transferwise/bootstrap#v5.18.15",
    "currency-flags": "^1.2.1",
    "prop-types": "^15.6.1",
    "react": "16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@emotion/core": "^10.0.14",
    "@transferwise/formatting": "^2.1.0",
    "@transferwise/icons": "^1.0.0",
    "@transferwise/neptune-css": "^0.13.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "lodash.clamp": "^4.0.3",
    "lodash.debounce": "^4.0.8",
    "lodash.topairs": "^4.3.0",
    "react-required-if": "^1.0.3",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.2.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": "@transferwise/eslint-config/.prettierrc.js"
}
